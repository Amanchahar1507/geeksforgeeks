class Solution{
public:	
		
	string removeVowels(string S) 
	{
	    string s1 = "";
	    //char n = {'a','e','i','o','u','A','E','I','O','U'};
	    for(int i=0;i<S.length();i++)
	    {
	        if(S[i]=='a'||S[i]=='e'||S[i]=='i'||S[i]=='o'||S[i]=='u')
	        {
	            continue;
	        }
	        else
	        s1.push_back(S[i]);
	    }
	    return s1;
	    // Your code goes here
	}
};
