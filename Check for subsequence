class Solution{
    public:
    bool isSubSequence(string A, string B) 
    {
       int i,n,j=0;
       //n1 = A.length();
       n = B.length();
       for(i=0;i<n;i++)
       {
           if(B[i]==A[j])
           {
               
               j++;
           }
           
           
          
       }
         if(j==A.length())
           return 1;
           else
           return 0;
    }
