link= https://practice.geeksforgeeks.org/problems/insertion-sort/1

 void insertionSort(int arr[], int n)
    {
        for(int i=1;i<n;i++){
            int number = arr[i];
            int j;
            for(j= i-1;j>=0;j--){
                if(arr[j]>number){
                    arr[j+1]= arr[j];
                }
                else{
                    break;
                }
            }
            arr[j+1] = number;
        }
        //code here
    }
