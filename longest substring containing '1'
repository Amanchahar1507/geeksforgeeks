int maxlength( string s)
{
        //string s="";
        int i,n,count=0,max=0;
        n = s.length();
        for(i=0;i<n;i++)
        {
            if(s[i]=='1')
            {
                count++;
                if(max<count)
                 {   
                  max = count;
                 } 
            }
             
            else
            count=0;
           
            
        }
        return max;
       
    
     
    // your code here
}
