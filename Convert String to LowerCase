class Solution {
  public:
    string toLower(string S) {
        int n;
        n=S.size();
        for(int i=0;i<n;i++)
        {
            if(S[i]<97)
            {
                cout<<char(S[i]+32);
            }
            else
            cout<<S[i];
            
        }
        return "";
        // code here
    }
};
